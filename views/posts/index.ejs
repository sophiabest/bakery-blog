<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% posts.forEach(function(p) { %>
      <tr>
        <td><%= p.title %></td>
        <td><%= p.content %></td>
        <td><a href="/posts/<%= p._id %>">Comments</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>